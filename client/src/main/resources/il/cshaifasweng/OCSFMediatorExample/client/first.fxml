<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="il.cshaifasweng.OCSFMediatorExample.client.firstController"
            prefWidth="600" prefHeight="400">
   <children>
      <TextField fx:id="IPtext"
                 layoutX="200" layoutY="100"
                 promptText="Enter IP"
                 prefWidth="200"/>
      <TextField fx:id="PortText"
                 layoutX="200" layoutY="180"
                 promptText="Enter Port"
                 prefWidth="200"/>
      <Button layoutX="250" layoutY="260"
              text="Start the game!"
              onAction="#StartTheGame"/>
   </children>
</AnchorPane>

